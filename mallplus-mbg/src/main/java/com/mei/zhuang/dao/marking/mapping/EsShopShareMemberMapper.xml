<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.arvato.service.marking.api.orm.dao.EsShopShareMemberMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mei.zhuang.entity.marking.EsShopShareMember">
        <id column="id" property="id" />
        <result column="share_id" property="shareId" />
        <result column="launch_member_id" property="launchMemberId" />
        <result column="create_time" property="createTime" />
        <result column="is_close" property="isClose"/>
        <result column="is_confirm" property="isConfirm"/>
    </resultMap>

    <select id="selListOrderBy" resultMap="BaseResultMap">
        select * from es_shop_share_member where share_id=#{shareId} and launch_member_id=#{launchMemberId} order by create_time desc limit 1 offset 0
    </select>

</mapper>
