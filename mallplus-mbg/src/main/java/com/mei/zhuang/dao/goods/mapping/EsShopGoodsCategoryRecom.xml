<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.arvato.service.goods.api.orm.dao.EsShopGoodsCategoryRecomMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mei.zhuang.entity.goods.EsShopGoodsCategoryRecom">
        <result column="category_id" property="categoryId"/>
        <result column="goods_id" property="goodsId"/>
    </resultMap>


    <select id="selCategoryRecom" resultType="com.mei.zhuang.entity.goods.EsShopGoodsCategoryRecom">
        select * from es_shop_goods_category_recom where category_id=#{categoryId}
    </select>

    <delete id="delete">
        delete from es_shop_goods_category_recom where category_id=#{categoryId}
    </delete>


</mapper>
