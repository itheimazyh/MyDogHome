<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mei.zhuang.dao.goods.EsShopGoodsGroupMapMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mei.zhuang.entity.goods.EsShopGoodsGroupMap">
        <result column="shop_id" property="shopId"/>
        <result column="goods_id" property="goodsId"/>
        <result column="group_id" property="groupId"/>
    </resultMap>
    <select id="selEsShopGoodsGroupMap" resultType="com.mei.zhuang.entity.goods.EsShopGoodsGroupMap">
        select * from es_shop_goods_group_map where group_id=#{id}
    </select>

</mapper>
