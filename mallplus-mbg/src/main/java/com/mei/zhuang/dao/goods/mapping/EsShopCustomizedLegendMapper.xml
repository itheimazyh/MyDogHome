<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.arvato.service.goods.api.orm.dao.EsShopCustomizedLegendMapper">

    <resultMap id="BaseResultMap" type="com.mei.zhuang.entity.goods.EsShopCustomizedLegend">
        <id column="id" property="id"/>
        <result column="title" property="title"/>
        <result column="content" property="content"/>
        <result column="img" property="img"/>
        <result column="write_color" property="writeColor"/>
        <result column="write_size" property="writeSize"/>
        <result column="write_show" property="writeShow"/>
        <result column="create_time" property="createTime"/>
        <result column="position" property="position"/>
    </resultMap>
    <select id="selLegendPage" resultType="com.mei.zhuang.entity.goods.EsShopCustomizedLegend">
        select * from es_shop_customized_legend
        order by create_time desc
    </select>

</mapper>
