<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mei.zhuang.dao.order.EsShopFormidMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mei.zhuang.entity.order.EsShopFormid">
        <id column="id" property="id" />
        <result column="member_id" property="memberId" />
        <result column="source" property="source" />
        <result column="status" property="status" />
        <result column="form_id" property="formId" />
        <result column="create_time" property="createTime" />
    </resultMap>
    <select id="getFormIdByMemberId" resultMap="BaseResultMap" >
        select * from es_shop_formid where member_id=#{userId} and status=1 and create_time>=TO_DATE(#{createTime}, 'YYYY-MM-DD')  order by create_time asc
    </select>
</mapper>
