<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mei.zhuang.dao.marking.EsShopCouponManualUserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mei.zhuang.entity.marking.EsShopCouponManualUser">
        <id column="id" property="id" />
        <result column="manual_id" property="manualId" />
        <result column="user_id" property="userId" />
        <result column="status" property="status" />
    </resultMap>


    <update id="updateuser">
        update es_shop_coupon_manual_user set status='2' where user_id=#{param1} and manual_id=#{param2}
    </update>

</mapper>
