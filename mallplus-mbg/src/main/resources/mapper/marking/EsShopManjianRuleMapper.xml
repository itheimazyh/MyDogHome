<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mei.zhuang.dao.marking.EsShopManjianRuleMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mei.zhuang.entity.marking.EsShopManjianRule">
        <result column="activity_type" property="activityType"/>
        <result column="manjian_id" property="manjianId"/>
        <result column="extra" property="extra"/>
        <result column="id" property="id"/>
        <result column="shop_id" property="shopId"/>
        <result column="total" property="total"/>
        <result column="consumption_amount" property="consumptionAmount"/>
        <result column="coupon_amount" property="couponAmount"/>
